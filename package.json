{
	"name": "node-chat",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"engines": {
		"node": "8.10.0",
		"npm": "6.1.0",
		"yarn": "1.7.0"
	},
	"dependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"bcrypt": "^3.0.0",
		"body-parser": "^1.18.3",
		"concurrently": "^3.6.0",
		"express": "^4.16.3",
		"express-jwt": "^5.3.1",
		"jsonwebtoken": "^8.3.0",
		"mongoose": "^5.2.1",
		"morgan": "^1.9.0",
		"nodemon": "^1.17.5",
		"socket.io": "^2.1.1"
	},
	"scripts": {
		"start:dev-server": "nodemon server.js --exec babel-node --presets es2015,stage-0",
		"start:client": "cd client && yarn webpack",
		"start:dev": "concurrently \"yarn start:dev-server\" \"yarn start:client\"",
		"start": "babel-node server.js",
		"heroku-postbuild": "cd client && yarn  && yarn run build"
	},
	"cacheDirectories": [
		"node_modules",
		"react-ui/node_modules"
	]
}